entity Report{
    generatedDate Instant required
}

entity Notification{
    message String maxlength(1000),
    isRead Boolean,
    notificationDate Instant required
}

entity MonthlyBudget{
    amount BigDecimal
}

entity SavingGoal{
    targetAmount BigDecimal,
    currentAmount BigDecimal,
    targetDate LocalDate
}

entity ExpenseCategory{
    name String
}

entity Expense{
    amount BigDecimal,
    date Instant,
    description String maxlength(1000)
}

relationship OneToMany{
    User{reports} to Report with builtInEntity,
    User{notifications} to Notification with builtInEntity,
    User{monthlyBudget} to MonthlyBudget with builtInEntity,
    User{expenses} to Expense with builtInEntity
}

relationship OneToMany{
    ExpenseCategory to MonthlyBudget{expenseCategory}
    ExpenseCategory to Expense{expenseCategory}
}





